{"ast":null,"code":"import { authApi, endpoints } from \"@/configs/Apis.js\";\nimport { mapGetters } from \"vuex\";\nimport { useMenu } from \"../../../stores/use-menu.js\";\nexport default {\n  computed: {\n    ...mapGetters([\"isAuth\", \"getUser\"])\n  },\n  data() {\n    return {\n      studentList: []\n    };\n  },\n  created() {\n    this.getStudent();\n  },\n  methods: {\n    async getStudent() {\n      console.log(this.getUser.username);\n      if (this.getUser && this.getUser.username) {\n        const lecturerUsername = this.getUser.username;\n        const response = await authApi().get(endpoints[\"get-lecturer-by-username\"].replace(\"{username}\", lecturerUsername));\n        console.log(response.data.id);\n        const listStudent = await authApi().get(endpoints[\"student-home-room-teacher\"].replace(\"{lecturerId}\", response.data.id));\n        if (listStudent.data) {\n          this.studentList = listStudent.data;\n          console.log(listStudent.data);\n        }\n      }\n    }\n  },\n  computed: {\n    extractedId() {\n      return this.studentList.length > 0 ? this.studentList[0].classesId?.id || \"N/A\" : \"N/A\";\n    }\n  },\n  setup() {\n    useMenu().onSelectedKeys([\"teacher-classStudent\"]);\n  }\n};","map":{"version":3,"names":["authApi","endpoints","mapGetters","useMenu","computed","data","studentList","created","getStudent","methods","console","log","getUser","username","lecturerUsername","response","get","replace","id","listStudent","extractedId","length","classesId","setup","onSelectedKeys"],"sources":["D:\\university\\src\\pages\\teacher\\classStudent\\index.vue"],"sourcesContent":["<template>\r\n  <div>Lớp: {{ extractedId }}</div>\r\n  <div class=\"table col-12\">\r\n    <div class=\"table\">\r\n      <template v-if=\"studentList.length > 0\">\r\n        <table class=\"table table-striped table-bordered table-hover\">\r\n          <thead>\r\n            <tr>\r\n              <th style=\"text-align: center\">Mã số sinh viên</th>\r\n              <th style=\"text-align: center\">Tên sinh viên</th>\r\n              <th style=\"text-align: center\">Ngày sinh</th>\r\n              <th style=\"text-align: center\">Giới tính</th>\r\n              <th style=\"text-align: center\">Số điện thoại</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"student in studentList\" :key=\"student.id\">\r\n              <td style=\"text-align: center\">{{ student.id }}</td>\r\n              <td style=\"text-align: center\">{{ student.name }}</td>\r\n              <td style=\"text-align: center\">\r\n                {{ formatDate(student.birthday) }}\r\n              </td>\r\n              <td style=\"text-align: center\">\r\n                {{ student.gender === 1 ? \"Nam\" : \"Nữ\" }}\r\n              </td>\r\n              <td style=\"text-align: center\">{{ student.phone }}</td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </template>\r\n      <template v-else>\r\n        <div class=\"text-hr-teacher\">Không có lớp chủ nhiệm</div>\r\n      </template>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { authApi, endpoints } from \"@/configs/Apis.js\";\r\nimport { mapGetters } from \"vuex\";\r\nimport { useMenu } from \"../../../stores/use-menu.js\";\r\nexport default {\r\n  computed: {\r\n    ...mapGetters([\"isAuth\", \"getUser\"]),\r\n  },\r\n  data() {\r\n    return {\r\n      studentList: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.getStudent();\r\n  },\r\n  methods: {\r\n    async getStudent() {\r\n      console.log(this.getUser.username);\r\n      if (this.getUser && this.getUser.username) {\r\n        const lecturerUsername = this.getUser.username;\r\n        const response = await authApi().get(\r\n          endpoints[\"get-lecturer-by-username\"].replace(\r\n            \"{username}\",\r\n            lecturerUsername\r\n          )\r\n        );\r\n\r\n        console.log(response.data.id);\r\n        const listStudent = await authApi().get(\r\n          endpoints[\"student-home-room-teacher\"].replace(\r\n            \"{lecturerId}\",\r\n            response.data.id\r\n          )\r\n        );\r\n\r\n        if (listStudent.data) {\r\n          this.studentList = listStudent.data;\r\n          console.log(listStudent.data);\r\n        }\r\n      }\r\n    },\r\n  },\r\n  computed: {\r\n    extractedId() {\r\n      return this.studentList.length > 0\r\n        ? this.studentList[0].classesId?.id || \"N/A\"\r\n        : \"N/A\";\r\n    },\r\n  },\r\n  setup() {\r\n    useMenu().onSelectedKeys([\"teacher-classStudent\"]);\r\n  },\r\n};\r\n</script>"],"mappings":"AAsCA,SAASA,OAAO,EAAEC,SAAQ,QAAS,mBAAmB;AACtD,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,OAAM,QAAS,6BAA6B;AACrD,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGF,UAAU,CAAC,CAAC,QAAQ,EAAE,SAAS,CAAC;EACrC,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,CAAC,CAAC;EACnB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,UAAUA,CAAA,EAAG;MACjBE,OAAO,CAACC,GAAG,CAAC,IAAI,CAACC,OAAO,CAACC,QAAQ,CAAC;MAClC,IAAI,IAAI,CAACD,OAAM,IAAK,IAAI,CAACA,OAAO,CAACC,QAAQ,EAAE;QACzC,MAAMC,gBAAe,GAAI,IAAI,CAACF,OAAO,CAACC,QAAQ;QAC9C,MAAME,QAAO,GAAI,MAAMf,OAAO,CAAC,CAAC,CAACgB,GAAG,CAClCf,SAAS,CAAC,0BAA0B,CAAC,CAACgB,OAAO,CAC3C,YAAY,EACZH,gBACF,CACF,CAAC;QAEDJ,OAAO,CAACC,GAAG,CAACI,QAAQ,CAACV,IAAI,CAACa,EAAE,CAAC;QAC7B,MAAMC,WAAU,GAAI,MAAMnB,OAAO,CAAC,CAAC,CAACgB,GAAG,CACrCf,SAAS,CAAC,2BAA2B,CAAC,CAACgB,OAAO,CAC5C,cAAc,EACdF,QAAQ,CAACV,IAAI,CAACa,EAChB,CACF,CAAC;QAED,IAAIC,WAAW,CAACd,IAAI,EAAE;UACpB,IAAI,CAACC,WAAU,GAAIa,WAAW,CAACd,IAAI;UACnCK,OAAO,CAACC,GAAG,CAACQ,WAAW,CAACd,IAAI,CAAC;QAC/B;MACF;IACF;EACF,CAAC;EACDD,QAAQ,EAAE;IACRgB,WAAWA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACd,WAAW,CAACe,MAAK,GAAI,IAC7B,IAAI,CAACf,WAAW,CAAC,CAAC,CAAC,CAACgB,SAAS,EAAEJ,EAAC,IAAK,KAAI,GACzC,KAAK;IACX;EACF,CAAC;EACDK,KAAKA,CAAA,EAAG;IACNpB,OAAO,CAAC,CAAC,CAACqB,cAAc,CAAC,CAAC,sBAAsB,CAAC,CAAC;EACpD;AACF,CAAC"},"metadata":{},"sourceType":"modul