{"ast":null,"code":"import { authApi, endpoints } from \"@/configs/Apis.js\";\nimport { mapGetters } from 'vuex';\nimport { useMenu } from '@/stores/use-menu';\nexport default {\n  computed: {\n    ...mapGetters([\"isAuth\", \"getUser\"])\n  },\n  data() {\n    return {\n      studentList: []\n    };\n  },\n  created() {\n    this.getStudent();\n  },\n  methods: {\n    async getStudent() {\n      const lecturerUsername = this.getUser.username;\n      const response = await authApi().get(endpoints[\"get-lecturer-by-username\"].replace(\"{username}\", lecturerUsername));\n      console.log(response.data.id);\n      const listStudent = await authApi().get(endpoints[\"student-home-room-teacher\"].replace(\"{lecturerId}\", response.data.id));\n      if (listStudent.data) {\n        this.studentList = listStudent.data;\n        console.log(listStudent.data);\n      }\n    }\n  },\n  computed: {\n    extractedId() {\n      return this.studentList.length > 0 ? this.studentList[0].classesId?.id || \"N/A\" : \"N/A\";\n    }\n  },\n  setup() {\n    useMenu().onSelectedKeys([\"teacher-classStudent\"]);\n  }\n};","map":{"version":3,"names":["authApi","endpoints","mapGetters","useMenu","computed","data","studentList","created","getStudent","methods","lecturerUsername","getUser","username","response","get","replace","console","log","id","listStudent","extractedId","length","classesId","setup","onSelectedKeys"],"sources":["D:\\university\\src\\pages\\teacher\\detailForum\\index.vue"],"sourcesContent":["<template>\r\n    <div class=\"container-fluid\">\r\n        <div class=\"row\">\r\n            <div className=\"container\">\r\n                <p className=\"postDetail\">{post.title}</p>\r\n                <p className=\"userDetail\">Đăng bởi: {usernameHost} - Thời gian: {formatDate(post.postTime)}</p>\r\n                <p>{post.content}</p>\r\n            </div>\r\n            <hr/>\r\n            <div class=\"post-container\">\r\n                <label for=\"comment\">Bình luận:</label>\r\n                <textarea class=\"form-control\" rows=\"5\" id=\"comment\" name=\"text\"\r\n                    placeholder=\"Nhập bình luận\"></textarea>\r\n                <Button class=\"btn-title btn btn-primary\">Gửi</Button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { authApi, endpoints } from \"@/configs/Apis.js\";\r\nimport { mapGetters } from 'vuex';\r\nimport { useMenu } from '@/stores/use-menu';\r\n\r\nexport default {\r\n  computed: {\r\n    ...mapGetters([\"isAuth\", \"getUser\"]),\r\n  },\r\n  data() {\r\n    return {\r\n      studentList: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.getStudent();\r\n  },\r\n  methods: {\r\n    async getStudent() {\r\n     \r\n        const lecturerUsername = this.getUser.username;\r\n        const response = await authApi().get(\r\n          endpoints[\"get-lecturer-by-username\"].replace(\r\n            \"{username}\",\r\n            lecturerUsername\r\n          )\r\n        );\r\n\r\n        console.log(response.data.id);\r\n        const listStudent = await authApi().get(\r\n          endpoints[\"student-home-room-teacher\"].replace(\r\n            \"{lecturerId}\",\r\n            response.data.id\r\n          )\r\n        );\r\n\r\n        if (listStudent.data) {\r\n          this.studentList = listStudent.data;\r\n          console.log(listStudent.data);\r\n        }\r\n      \r\n    },\r\n  },\r\n  computed: {\r\n    extractedId() {\r\n      return this.studentList.length > 0\r\n        ? this.studentList[0].classesId?.id || \"N/A\"\r\n        : \"N/A\";\r\n    },\r\n  },\r\n  setup() {\r\n    useMenu().onSelectedKeys([\"teacher-classStudent\"]);\r\n  },\r\n};\r\n</script>"],"mappings":"AAoBA,SAASA,OAAO,EAAEC,SAAQ,QAAS,mBAAmB;AACtD,SAASC,UAAS,QAAS,MAAM;AACjC,SAASC,OAAM,QAAS,mBAAmB;AAE3C,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGF,UAAU,CAAC,CAAC,QAAQ,EAAE,SAAS,CAAC;EACrC,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO