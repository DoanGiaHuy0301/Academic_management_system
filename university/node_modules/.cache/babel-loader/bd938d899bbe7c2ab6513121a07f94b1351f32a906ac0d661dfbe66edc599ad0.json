{"ast":null,"code":"import { useMenu } from '../../stores/use-menu.js';\nimport TheHeader from '../../components/TheHeader.vue';\nimport TheFooter from '../../components/TheFooter.vue';\nimport Apis, { endpoints, authApi } from '../../configs/Apis.js';\nimport { mapGetters } from \"vuex\";\nexport default {\n  components: {\n    TheHeader,\n    TheFooter\n  },\n  computed: {\n    ...mapGetters([\"isAuth\", \"getUser\"])\n  },\n  data() {\n    return {\n      user: {},\n      userInfo: {},\n      formattedBirthday: \"\"\n    };\n  },\n  created() {\n    this.fetchUserInfo();\n  },\n  methods: {\n    async fetchUserInfo() {\n      let endpoint = \"\";\n      const userRole = this.getUser.role;\n      console.log(userRole);\n      if (userRole === \"ROLE_SINHVIEN\") {\n        endpoint = endpoints['get-student-by-username'];\n      } else if (userRole === \"ROLE_GIANGVIEN\") {\n        endpoint = endpoints['get-lecturer-by-username'];\n      }\n      console.log(this.getUser.username);\n      const response = await authApi().get(endpoint.replace(\"{username}\", this.getUser.username));\n      this.userInfo = response.data;\n      if (response.data.birthday) {\n        const birthdayTimestamp = response.data.birthday;\n        const birthdayDate = new Date(birthdayTimestamp);\n        const formattedBirthday = `${birthdayDate.getDate()}/${birthdayDate.getMonth() + 1}/${birthdayDate.getFullYear()}`;\n        this.formattedBirthday = formattedBirthday;\n      }\n    }\n  },\n  setup() {\n    useMenu().onSelectedKeys([\"teacher-profile\"]);\n  }\n};","map":{"version":3,"names":["useMenu","TheHeader","TheFooter","Apis","endpoints","authApi","mapGetters","components","computed","data","user","userInfo","formattedBirthday","created","fetchUserInfo","methods","endpoint","userRole","getUser","role","console","log","username","response","get","replace","birthday","birthdayTimestamp","birthdayDate","Date","getDate","getMonth","getFullYear","setup","onSelectedKeys"],"sources":["D:\\university\\src\\pages\\profile\\index.vue"],"sourcesContent":["<template>\r\n    <TheHeader />\r\n    <section class=\"section about-section gray-bg\" id=\"about\">\r\n        <div class=\"container\">\r\n          <div class=\"row align-items-center flex-row-reverse\">\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"about-text go-to\">\r\n                <h3 class=\"dark-color\">Giới thiệu</h3>\r\n                <h6 class=\"theme-color lead\">Mô tả</h6>\r\n                <p>I <mark>design and develop</mark> services for customers of all sizes, specializing in creating stylish, modern websites, web services and online stores. My passion is to design digital user experiences through the bold interface and meaningful interactions.</p>\r\n                <div class=\"row about-list\">\r\n                  <div class=\"col-md-6\">\r\n                    <div class=\"media\">\r\n                      <label>Tên</label>\r\n                      <p>{{ userInfo.name }}</p>\r\n                    </div>\r\n                    <div class=\"media\">\r\n                      <label>Ngày sinh</label>\r\n                      <p>{{ formattedBirthday }}</p>\r\n                    </div>\r\n                    <div class=\"media\">\r\n                      <label>Địa chỉ</label>\r\n                      <p>{{ userInfo.address }}</p>\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"col-md-6\">\r\n                    <div class=\"media\">\r\n                      <label>E-mail</label>\r\n                      <p>{{ user.email }}</p>\r\n                    </div>\r\n                    <div class=\"media\">\r\n                      <label>Số điện thoại</label>\r\n                      <p>{{ userInfo.phone }}</p>\r\n                    </div>\r\n                    <div class=\"media\">\r\n                      <label>Khoa</label>\r\n                      <p v-if=\"userInfo.facultyId\">{{ userInfo.facultyId.name }}</p>\r\n                    </div>\r\n                    <div class=\"media\">\r\n                      <label>Chuyên ngành</label>\r\n                      <p v-if=\"userInfo.majorId\">{{ userInfo.majorId.name }}</p>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-lg-6\">\r\n              <div class=\"about-avatar text-center\">\r\n                <img :src=\"user.avatar\" title=\"\" alt=\"\" />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    <TheFooter />\r\n</template>\r\n\r\n<script>\r\n    import { useMenu } from '../../stores/use-menu.js';\r\n    import TheHeader from '../../components/TheHeader.vue'\r\n    import TheFooter from '../../components/TheFooter.vue'\r\n    import Apis, { endpoints, authApi } from '../../configs/Apis.js';\r\n    import { mapGetters } from \"vuex\";\r\n    export default {\r\n        components: {\r\n            TheHeader,\r\n            TheFooter,\r\n        },\r\n        computed: {\r\n            ...mapGetters([\"isAuth\", \"getUser\"]),\r\n        },\r\n        data() {\r\n            return {\r\n                user: {},\r\n                userInfo: {},\r\n                formattedBirthday: \"\"\r\n            };\r\n        },\r\n        created() {\r\n            this.fetchUserInfo();\r\n        },\r\n        methods: {\r\n            async fetchUserInfo() {\r\n                let endpoint = \"\";\r\n                const userRole = this.getUser.role;\r\n                console.log(userRole);\r\n                if (userRole === \"ROLE_SINHVIEN\") {\r\n                    endpoint = endpoints['get-student-by-username'];\r\n                } else if (userRole === \"ROLE_GIANGVIEN\") {\r\n                    endpoint = endpoints['get-lecturer-by-username'];\r\n                }\r\n                console.log(this.getUser.username);\r\n                const response = await authApi().get(endpoint.replace(\"{username}\", this.getUser.username));\r\n                this.userInfo = response.data;\r\n\r\n                if (response.data.birthday) {\r\n                    const birthdayTimestamp = response.data.birthday;\r\n                    const birthdayDate = new Date(birthdayTimestamp);\r\n\r\n                    const formattedBirthday = `${birthdayDate.getDate()}/${birthdayDate.getMonth() + 1}/${birthdayDate.getFullYear()}`;\r\n                    this.formattedBirthday = formattedBirthday;\r\n                }\r\n            }\r\n        },\r\n        setup() {\r\n            useMenu().onSelectedKeys([\"teacher-profile\"]);\r\n\r\n        },\r\n    }\r\n\r\n</script>"],"mappings":"AA0DI,SAASA,OAAM,QAAS,0BAA0B;AAClD,OAAOC,SAAQ,MAAO,gCAA+B;AACrD,OAAOC,SAAQ,MAAO,gCAA+B;AACrD,OAAOC,IAAI,IAAIC,SAAS,EAAEC,OAAM,QAAS,uBAAuB;AAChE,SAASC,UAAS,QAAS,MAAM;AACjC,eAAe;EACXC,UAAU,EAAE;IACRN,SAAS;IACTC;EACJ,CAAC;EACDM,QAAQ,EAAE;IACN,GAAGF,UAAU,CAAC,CAAC,QAAQ,EAAE,SAAS,CAAC;EACvC,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,CAAC,CAAC;MACRC,QAAQ,EAAE,CAAC,CAAC;MACZC,iBAAiB,EAAE;IACvB,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,aAAa,CAAC,CAAC;EACxB,CAAC;EACDC,OAAO,EAAE;IACL,MAAMD,aAAaA,CAAA,EAAG;MAClB,IAAIE,QAAO,GAAI,EAAE;MACjB,MAAMC,QAAO,GAAI,IAAI,CAACC,OAAO,CAACC,IAAI;MAClCC,OAAO,CAACC,GAAG,CAACJ,QAAQ,CAAC;MACrB,IAAIA,QAAO,KAAM,eAAe,EAAE;QAC9BD,QAAO,GAAIZ,SAAS,CAAC,yBAAyB,CAAC;MACnD,OAAO,IAAIa,QAAO,KAAM,gBAAgB,EAAE;QACtCD,QAAO,GAAIZ,SAAS,CAAC,0BAA0B,CAAC;MACpD;MACAgB,OAAO,CAACC,GAAG,CAAC,IAAI,CAACH,OAAO,CAACI,QAAQ,CAAC;MAClC,MAAMC,QAAO,GAAI,MAAMlB,OAAO,CAAC,CAAC,CAACmB,GAAG,CAACR,QAAQ,CAACS,OAAO,CAAC,YAAY,EAAE,IAAI,CAACP,OAAO,CAACI,QAAQ,CAAC,CAAC;MAC3F,IAAI,CAACX,QAAO,GAAIY,QAAQ,CAACd,IAAI;MAE7B,IAAIc,QAAQ,CAACd,IAAI,CAACiB,QAAQ,EAAE;QACxB,MAAMC,iBAAgB,GAAIJ,QAAQ,CAACd,IAAI,CAACiB,QAAQ;QAChD,MAAME,YAAW,GAAI,IAAIC,IAAI,CAACF,iBAAiB,CAAC;QAEhD,MAAMf,iBAAgB,GAAK,GAAEgB,YAAY,CAACE,OAAO,CAAC,CAAE,IAAGF,YAAY,CAACG,QAAQ,CA