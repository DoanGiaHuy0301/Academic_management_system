{"ast":null,"code":"import { authApi, endpoints } from \"@/configs/Apis.js\";\nimport { mapGetters } from \"vuex\";\nimport { useMenu } from \"../../../stores/use-menu.js\";\nexport default {\n  computed: {\n    ...mapGetters([\"isAuth\", \"getUser\"])\n  },\n  data() {\n    return {\n      studentList: []\n    };\n  },\n  created() {\n    this.getStudent();\n  },\n  methods: {\n    async getStudent() {\n      const response = await authApi().get(endpoints[\"get-lecturer-by-username\"].replace(\"{username}\", this.getUser.username));\n      console.log(response.data.id);\n      const listStudent = await authApi().get(endpoints[\"student-home-room-teacher\"].replace(\"{lecturerId}\", response.data.id));\n      if (listStudent.data) {\n        console.log(listStudent.data);\n        this.studentList = listStudent.data;\n      }\n    }\n  },\n  setup() {\n    useMenu().onSelectedKeys([\"teacher-classStudent\"]);\n  }\n};","map":{"version":3,"names":["authApi","endpoints","mapGetters","useMenu","computed","data","studentList","created","getStudent","methods","response","get","replace","getUser","username","console","log","id","listStudent","setup","onSelectedKeys"],"sources":["D:\\university\\src\\pages\\teacher\\classStudent\\index.vue"],"sourcesContent":["<template>\r\n  <div>Lớp:</div>\r\n  <div className=\"table col-12\">\r\n    <Table style=\"margin: 0; width: 100%\">\r\n      <thead class=\"text-white\" style=\"background-color: #0c713d\">\r\n        <tr>\r\n          <th style=\"align-items: center\">Mã số sinh viên</th>\r\n          <th style=\"align-items: center\">Tên sinh viên</th>\r\n          <th style=\"align-items: center\">Ngày sinh</th>\r\n          <th style=\"align-items: center\">Giới tính</th>\r\n          <th style=\"align-items: center\">Số điện thoại</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"student in studentList\" :key=\"student.id\">\r\n          <td style=\"text-align: center\">{{ student.id }}</td>\r\n  